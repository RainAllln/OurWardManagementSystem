<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="team.jnu.wardsystem.mapper.UserMapper">
    <select id="selectAll" resultType="User"> select * from users; </select>
    <select id="selectByUserName" resultType="User"> select * from users where user_name=#{user_name}; </select>
    <select id="confirmPassword" resultType="string"> select password from users where user_name=#{user_name} </select>

</mapper>
