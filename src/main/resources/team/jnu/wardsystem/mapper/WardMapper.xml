<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="team.jnu.wardsystem.mapper.WardMapper">
    <select id="searchWardById" resultType="team.jnu.wardsystem.pojo.Ward">
        select * from wards where ward_id=#{ward_id}
    </select>
    <select id="searchWardType" resultType="java.lang.String">
        select ward_type from wards where ward_id=#{ward_id}
    </select>
</mapper>